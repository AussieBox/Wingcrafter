{
  "schemaVersion": 1,
  "id": "wingcrafter",
  "version": "${version}",
  "name": "Wingcrafter",
  "description": "Bring the world of Wings of Fire into Minecraft, with many interactive and exciting features!",
  "authors": ["AussieBox (aka. Stargazer)"],
  "contact": {
    "homepage": "https://modrinth.com/project/wingcrafter",
    "sources": "https://github.com/AussieBox/Wingcrafter",
    "issues": "https://github.com/AussieBox/Wingcrafter/issues"
  },
  "license": "MIT",
  "icon": "assets/wingcrafter/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "org.aussiebox.wingcrafter.client.WingcrafterClient",
      "org.aussiebox.wingcrafter.client.HudRenderingEntrypoint"
    ],
    "main": [
      "org.aussiebox.wingcrafter.Wingcrafter"
    ],
    "fabric-datagen": [
      "org.aussiebox.wingcrafter.client.WingcrafterClient"
    ],
    "modmenu": [
      "org.aussiebox.wingcrafter.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "wingcrafter.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "yet_another_config_lib_v3": ">=3.8.0+1.21.9-fabric",
    "fabric-key-binding-api-v1": "*"
  }
}
